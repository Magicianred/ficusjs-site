<div class="blog-posts">
  {%- for post in blogPages -%}
    <div class="feature">
      <h2>{{ post.title }}</h2>
      <p>{{ post.author }}</p>
      <p>{{ post.date }}</p>
      <p>{{ post.description }}</p>
      <p><a href="{{ post.url }}" class="fc-button">Read more</a></p>
    </div>
  {%- endfor -%}
</div>
